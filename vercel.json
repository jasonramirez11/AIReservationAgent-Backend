{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "functions": {
      "api/call-response": {
        "memory": 1024,
        "maxDuration": 10
      },
      "api/call-status": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }